<div id="<%= dom_id experience %>">
  <p>
    <strong>Category:</strong>
    <%= experience.category %>
  </p>

  <p>
    <strong>Title:</strong>
    <%= experience.title %>
  </p>

  <p>
    <strong>Name:</strong>
    <%= experience.name %>
  </p>

  <p>
    <strong>Location:</strong>
    <%= experience.location %>
  </p>

  <p>
    <strong>Content:</strong>
    <%= experience.content %>
  </p>

  <p>
    <strong>Started at:</strong>
    <%= experience.started_at %>
  </p>

  <p>
    <strong>Ended at:</strong>
    <%= experience.ended_at %>
  </p>

</div>
